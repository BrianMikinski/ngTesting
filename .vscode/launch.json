{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ngTesting",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/wwwroot/index.html",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}/wwwroot",
            "diagnosticLogging": true
        },
        {
            "name": "Attach",
            "type": "chrome",
            "request": "attach",
            "port":9222,
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/wwwroot",
            "url":"http://localhost:8080"
        }
    ]
}